Container With Most Water

    int maxArea(vector<int>& height) {
        int start=0;
        int end=height.size()-1;
        int area;
        int maxArea=0;
        while(start<end)
        {
            area=(min(height[start],height[end]))*(end-start);
            if(area>maxArea)
                maxArea=area;
            if(height[start]<height[end]) start++;
            else end--;
        }
        return maxArea;
    }
